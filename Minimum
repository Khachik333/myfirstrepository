package arrays;

public class Minimum {
    public static void main(String [] args){
        int n;
        int[] minArray = new int[n];
        int min = minArray[0];
        for(int i = 0; i != minArray.length; i++){
            if(minArray[i] < min){
                min = minArray[i];
            }
        }
        System.out.println (min);
    }
}
