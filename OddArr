package arrays;

public class OddArray{

    public static void main(String [] args){
        printArray(createArray());
    }

    private static int[] createArray(){
        int[] odd = new odd[10];
        int n = 1;
        for(int i = 0; i < 10; i++){
            odd[i] = n;
            n += 2;
        }
        return odd;
    }

    private static void printArray(int[] array){
            for(int i = 0; i < 10; i++){
                System.out.println(array[i]);
            }
    }
}
